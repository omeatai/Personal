    <!-- Page Content-->
    <section>
      <div class="container px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5">
          <div class="col-lg-12">
            <div class="row mt-4">
              <div class="col-sm-6">
                <div class="card text-white bg-primary mb-3">
                  <div class="card-body">
                    <h5 class="card-title">User List</h5>
                    <p class="card-text">
                      View the List of all the Created Users.
                    </p>
                    <a href="/userlist" class="btn btn-dark">Go to User List</a>
                  </div>
                </div>
              </div>
              <div class="col-sm-6">
                <div class="card text-white bg-danger mb-3">
                  <div class="card-body">
                    <h5 class="card-title">Video List</h5>
                    <p class="card-text">View the List of all the Videos.</p>
                    <a href="/videolist" class="btn btn-dark"
                      >Go to Video List</a
                    >
                  </div>
                </div>
              </div>
            </div>

            <div class="row mt-4">
              <div class="col-sm-6">
                <div class="card text-white bg-success mb-3">
                  <div class="card-body">
                    <h5 class="card-title">Create User</h5>
                    <p class="card-text">Create and store a User in the DB.</p>
                    <a href="/usercreate" class="btn btn-dark"
                      >Go to Create User</a
                    >
                  </div>
                </div>
              </div>
              <div class="col-sm-6">
                <div class="card text-white bg-secondary mb-3">
                  <div class="card-body">
                    <h5 class="card-title">Update Database</h5>
                    <p class="card-text">
                      Update database with details of all the current stored
                      Videos.
                    </p>
                    <a href="/updatedb" class="btn btn-dark"
                      >Go to Update Database</a
                    >
                  </div>
                </div>
              </div>
            </div>

            <div class="row mt-4">
              <div class="col-sm-6">
                <div class="card text-white bg-secondary mb-3">
                  <div class="card-body">
                    <h5 class="card-title">Admin Logout</h5>
                    <p class="card-text">Logout from Admin Console.</p>
                    <a href="/adminlogout" class="btn btn-dark">Logout</a>
                  </div>
                </div>
              </div>
              <div class="col-sm-6">
