<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/dashboardhead.ejs') %>
  </head>
  <body>
    <!-- Navigation-->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-bottom">
      <%- include('./partials/dashboardnav.ejs') %>
    </nav>
    <!-- Page Content-->
    <section>
      <div class="container px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5">
          <div class="col-lg-12">
            <h1 class="dashboard-title">User List</h1>
            <p class="dashboard-subtitle">List of all the Created Users.</p>
            <div class="row">
              <table class="table table-striped">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">id</th>
                    <th scope="col">Firstname</th>
                    <th scope="col">Lastname</th>
                    <th scope="col">Username</th>
                    <th scope="col">Passcode</th>
                    <th scope="col">IPaddress</th>
                    <th scope="col">Created</th>
                    <th scope="col">Duration</th>
                    <th scope="col">Expiry</th>
                    <th scope="col">Status</th>
                  </tr>
                </thead>
                <tbody>
                  <% result.forEach(function(user) { %>
                  <tr>
                    <th scope="row"><%= user.count %></th>
                    <td><%= user.id %></td>
                    <td><%= user.firstname %></td>
                    <td><%= user.lastname %></td>
                    <td><%= user.username %></td>
                    <td><%= user.passcode %></td>
                    <td><%= user.ipaddress %></td>
                    <td><%= user.created %></td>
                    <td><%= user.duration %></td>
                    <td><%= user.expiry %></td>
                    <td><%= user.status %></td>
                  </tr>
                  <% }); %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </section>
    <%- include('./partials/dashboardfooter.ejs') %>
  </body>
</html>
